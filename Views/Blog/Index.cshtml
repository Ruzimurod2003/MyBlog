@{
    ViewBag.Title="My Blog";
    Layout="_Layout.cshtml";
}
@using MyBlog.Models
@model List<User>
<table class="table table-bordered">
    <thead>
        <tr>
            <td>FullName</td>
            <td>EmailAdress</td>
            <td>Role</td>
            <td>Birth</td>
            <td>UpdateTime</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FullName</td>
                <td>@item.EmailAdress</td>
                <td>@item.Role</td>
                <td>@item.Birth</td>
                <td>@item.UpdateTime</td>
            </tr>
        }
    </tbody>
</table>